@misc{mahr_mit_2024,
	address = {Historisches Seminar der Universität Zürich},
	type = {Seminar Session},
	title = {Mit {ChatGPT} {Texte} schreiben: {Prompting}-{Methoden} für {Historiker}:innen},
	copyright = {Creative Commons Attribution Share Alike 4.0 International},
	shorttitle = {Mit {ChatGPT} {Texte} schreiben},
	url = {https://zenodo.org/doi/10.5281/zenodo.10815218},
	abstract = {Dieser Workshop bietet einen umfassenden Überblick über die Anwendung generativer KI-Systeme im Bereich der Geschichtswissenschaften. Zielgruppe sind Forschende und Studierende mit Interesse an der Schnittstelle von Digital Humanities und historischer Forschung. Der Workshop fokussiert sich insbesondere auf die Nutzung von textgenerierenden KI-Systemen wie OpenAI's ChatGPT, Google Gemini und Anthropic Claude.

Die Teilnehmenden sollen ein grundlegendes Verständnis dafür entwickeln, wie generative KI funktioniert, und lernen, diese effektiv für historische Forschungsarbeiten einzusetzen. Der Workshop deckt verschiedene Aspekte ab, einschliesslich der Funktionsweise von Sprachmodellen, Methoden des Promptings und den praktischen Umgang mit generativen KI-Systemen.

Ein besonderer Fokus liegt auf der Interaktion mit diesen Systemen. Dazu wird das Konzept des Promptings ausführlich erläutert, einschliesslich praktischer Tipps und Techniken, um die Genauigkeit und Relevanz der generierten Texte zu maximieren. Der Workshop beleuchtet ebenfalls die Grenzen dieser Technologien, wie die Schwierigkeit, Fiktion von Realität zu unterscheiden, und schliesst mit einem umfassenden Einblick in die Anwendungsbereiche generativer KI in der Geschichtswissenschaft.

Dieser Workshop bietet eine wertvolle Ressource für Historiker:innen und Interessierte, die sich mit den neuesten Entwicklungen im Bereich der digitalen Technologien in den Geisteswissenschaften auseinandersetzen möchten.},
	language = {de},
	urldate = {2025-03-12},
	author = {Mähr, Moritz},
	month = mar,
	year = {2024},
	note = {Publisher: Zenodo},
	keywords = {Digital History, Digital Humanities, Generative KI, Geschichtswissenschaft, Large Language Model, LLM, Prompting, Prompting-Methoden, Prompting-Strategien, Sprachmodelle, Textgeneratoren},
}

@misc{mahr_mit_2024-1,
	address = {Historisches Seminar der Universität Zürich},
	title = {Mit {ChatGPT} {Texte} schreiben: {Prompting}-{Methoden} für {Historiker}:innen},
	copyright = {Creative Commons Attribution Share Alike 4.0 International},
	shorttitle = {Mit {ChatGPT} {Texte} schreiben},
	url = {https://doi.org/10.5281/zenodo.10823269},
	abstract = {Die Präsentation „Mit ChatGPT Texte schreiben: Prompting-Methoden für Historiker:innen“ zielt darauf ab, den Teilnehmenden ein tiefes Verständnis über die Funktion und Anwendung von generativen KI-Textgeneratoren zu vermitteln. Zunächst werden Lernziele definiert, wie das Erlangen einer Intuition über die Funktionsweise generativer KI und das Verständnis ihrer Grenzen in der Textproduktion. Die Präsentation fokussiert auf populäre Textgeneratoren wie OpenAI ChatGPT, Google Gemini und Anthropic Claude.

Zu den Grundlagen gehören Erklärungen darüber, wie ein Sprachmodell auf Benutzereingaben reagiert und Text generiert. Hier wird das Beispiel von GPT-3 herangezogen, um zu illustrieren, wie solche Modelle trainiert und ausgeführt werden. Es folgt ein Abschnitt über die Möglichkeiten der Interaktion mit Textgeneratoren, wobei auf die Bedeutung klarer und präziser Kommunikation hingewiesen wird.

Ein wesentlicher Bestandteil der Präsentation ist die Einführung eines fiktiven Charakters namens Jason, der als metaphorische Repräsentation eines generativen KI-Modells dient. Jason symbolisiert die Stärken und Grenzen von KI-Modellen in der Texterstellung und Interaktion. Es werden konkrete Beispiele für das, was Jason kann und nicht kann, angeführt, wie das Schreiben kurzer Texte und das Unvermögen, zwischen Fiktion und Realität zu unterscheiden.

Die Präsentation schliesst mit detaillierten Anleitungen zum effektiven Prompting und interaktiven Übungen. Sie bietet auch eine Liste von Ressourcen für weiterführende Informationen und eine umfassende Bibliographie. Insgesamt ist die Präsentation eine gründliche Einführung in die Nutzung generativer KI für Textproduktion, speziell ausgerichtet auf die Bedürfnisse von Historiker:innen.},
	language = {de},
	urldate = {2025-03-12},
	author = {Mähr, Moritz},
	month = mar,
	year = {2024},
	note = {Publisher: Zenodo},
	keywords = {Digital History, Digital Humanities, Generative KI, Geschichtswissenschaft, Large Language Model, LLM, Prompting, Prompting-Methoden, Prompting-Strategien, Sprachmodelle, Textgeneratoren},
}

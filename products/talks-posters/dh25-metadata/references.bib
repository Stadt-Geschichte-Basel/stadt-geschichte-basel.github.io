}

@misc{quadeBewahrungZurOnlineVeroeffentlichung2024,
	title = {Von der {Bewahrung} zur {Online}-{Veröffentlichung} in {Museen} und {Sammlungen}. {Theorie} und mögliche {Praxis} des ethischen {Umgangs} mit {Daten} über {Sammlungsgut} aus kolonialen {Kontexten}},
	copyright = {Creative Commons Attribution 4.0 International},
	url = {https://doi.org/10.5281/zenodo.12732539},
	abstract = {Sammlungsgut aus kolonialen Kontexten findet sich in vielen deutschen Kulturerbeeinrichtungen. Debatten um Rückgabeforderungen und die Errichtung des Humboldt-Forums in Berlin haben in den letzten Jahren zu mehr Aufmerksamkeit für jene Kulturgüter in der Öffentlichkeit geführt. Ethische Aspekte beim Umgang mit ihnen rücken in der Museumsarbeit in den Fokus. Mit den CARE-Prinzipien wurde 2019 ein Konzept geschaffen, das die Beteiligung der Herkunftsgesellschaften vorsieht und sich an Institutionen richtet, die „indigene Daten” verwalten. Doch vor welchen Herausforderungen stehen Museen und Sammlungen, wenn sie diese Aspekte bei ihrer Arbeit berücksichtigen wollen? In diesem Beitrag werden theoretische Empfehlungen den praktischen Ansätzen aus sechs Einrichtungen gegenübergestellt, die sich mit diesen Fragen intensiv beschäftigt haben. Sie können als Anregung dienen, welche Wege des ethischen Umgangs es beim Sammeln, Bewahren, Ausstellen, Vermitteln und Forschen geben kann und welche Fragestellungen hierbei zu berücksichtigen sind.

Collection items from colonial contexts can be found in many German cultural heritage institutions. In recent years, debates about restitution claims and the establishment of the Humboldt Forum in Berlin have led to greater public awareness of these cultural assets. Ethical aspects in dealing with them are moving centre stage in museum work. With the CARE Principles, a concept was created in 2019 that provides for the participation of communities of origin and is aimed at institutions that manage ‘indigenous data’. But what challenges do museums and collections face if they want to take these aspects into account in their work? This article compares theoretical recommendations with practical approaches from six institutions that have worked intensively on these issues. They can serve as an inspiration as to what ethical approaches can be taken when collecting, preserving, exhibiting, communicating and researching and what issues need to be taken into account.},
	language = {de},
	urldate = {2025-05-09},
	author = {Quade, Lisa},
	collaborator = {Rohde-Enslin, Stefan and Marchini, Chiara and Städtler, Domenic and von Hagel, Frank},
	month = oct,
	year = {2024},
	note = {Publisher: Zenodo},
	keywords = {Museum, Koloniale Kontexte, Sensible Sammlungsgüter, Ethik, CARE},
}


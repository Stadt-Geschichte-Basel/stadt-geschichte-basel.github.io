@article{lemaire_vereinbarkeit_2018,
	title = {Vereinbarkeit von {Forschungsprozess} und {Datenmanagement} in den {Geisteswissenschaften}: {Forschungsdatenmanagement} nüchtern betrachtet},
	volume = {5},
	copyright = {Creative Commons Attribution 4.0 International},
	url = {https://doi.org/10.5282/o-bib/2018H4S237-247},
	doi = {10.5282/o-bib/2018H4S237-247},
	abstract = {In den Geisteswissenschaften stößt der Terminus Forschungsdatenmanagement auf viel Unverständnis und erscheint den Forschenden unvereinbar mit ihrer alltäglichen Arbeit. Der hiesige Beitrag zeigt anhand von Anwendungsbeispielen mit der Virtuellen Forschungsumgebung FuD, dass Datenmanagement dem Forschungsprozess innewohnt. Bei der Überführung der traditionellen Forschungsarbeit ins Digitale handelt es sich um eine Neuinterpretation des Bekannten, wobei jedoch viele Aspekte bereits in der Planungsphase expliziter gemacht werden müssen, was allerdings der Schärfung des Forschungsprozesses zuträglich ist. Zudem verfügen die Forschenden der Geisteswissenschaften für diesen Transformationsprozess aufgrund ihrer Ausbildung bereits über viele der dafür notwendigen Fähigkeiten.},
	language = {de},
	number = {4},
	urldate = {2025-01-24},
	journal = {o-bib. Das offene Bibliotheksjournal},
	author = {Lemaire, Marina},
	month = dec,
	year = {2018},
	pages = {237--247},
}
